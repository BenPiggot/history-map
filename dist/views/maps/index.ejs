<% include ../partials/header %>

<body>
  <!-- Remeber to put all the content you want on top of the slider below the slider code -->
  <div id='mapid'></div>
  <div class='shadow-cover'>
    <div class='map-index-header'>Explore the World</div>
  </div>

  <div class="row maps-list">
    <div class="col-sm-3">
      <h2>Maps</h2>
      <% mapList.forEach( function(map) { %>
        <div><%= map.latitude %></div>
      <% }) %>
    </div>

    <div class="col-sm-6">
      <button class="btn btn-info-outline btn-block btn-square">Create a New Map</button>
    </div>
  </div>

  <script src="https://unpkg.com/leaflet@1.0.2/dist/leaflet.js"></script>
  <script src='https://api.tiles.mapbox.com/mapbox.js/v2.1.9/mapbox.js'></script>

  <script src="http://code.jquery.com/jquery-latest.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/tether/1.4.0/js/tether.min.js"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>
  <script type="text/javascript" src="/js/maps.bundle.js"></script>
</body>
</html>